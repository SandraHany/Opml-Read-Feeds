@page
@model WebApplication1.Pages.RssModel
@{
    ViewData["Title"] = "RSS";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0" class="table">
        <thead class="thead-dark">
            <tr>
                <th>Guid</th>
                <th>PubDate</th>
                <th>Link</th>
            </tr>
        </thead>
        @foreach (var rssxml in Model.RssList)
        {
            <tr>
                <td>@rssxml.Guid</td>
                <td>@rssxml.PubDate</td>
                <td>@rssxml.Link</td>
            </tr>
        }
    </table>
</body>
</html>